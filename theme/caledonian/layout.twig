{% extends 'base.twig' %}

{% block layout %}
<div class="row">
{{ parent() }}

<div class="large-8 columns">
	<div class="row">
		
	{% block contenido %}
	{% endblock %}
		
</div>
</div>

<div class="large-4 columns hide-for-small">
	{% block barra_lateral %}
